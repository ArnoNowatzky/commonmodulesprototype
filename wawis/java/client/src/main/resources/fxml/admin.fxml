<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.GridPane?>
<GridPane fx:id="root" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="de.noventi.cm.wawi.java.AdminController" hgap="5" vgap="5">
    <padding><Insets top="40" bottom="40" left="30" right="30"/> </padding>

    <ComboBox fx:id="cboRuntimeType" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <GridPane.margin>
            <Insets top="30"/>
        </GridPane.margin>
    </ComboBox>
    <Button fx:id="btnInstall" text="Install all modules" GridPane.columnIndex="0" GridPane.rowIndex="1"></Button>
    <Button fx:id="btnStart" text="Start all modules" GridPane.columnIndex="0" GridPane.rowIndex="2"></Button>
    <Button fx:id="btnStop" text="Stop all modules" GridPane.columnIndex="0" GridPane.rowIndex="3"></Button>
    <Button fx:id="btnStatus" text="Get status" GridPane.columnIndex="0" GridPane.rowIndex="4"></Button>
    <TableView fx:id="tabModules" GridPane.columnIndex="0" GridPane.rowIndex="5" GridPane.fillWidth="true" GridPane.hgrow="ALWAYS"></TableView>



</GridPane>